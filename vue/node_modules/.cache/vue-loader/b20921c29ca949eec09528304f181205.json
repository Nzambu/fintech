       <v-card-title\n                class=\"px-10 py-0\"\n            >\n                <v-row>\n                    <v-col\n                        lg=\"3\" \n                        md=\"3\"\n                        sm=\"0\"\n                        class=\"d-none d-md-flex\"\n                    >\n                         <v-list-item two-line>\n                            <v-list-item-content>\n                                <v-list-item-title class=\"grey--text\">{{ title }}</v-list-item-title>\n                            </v-list-item-content>\n                        </v-list-item>\n                    </v-col>\n                    <v-col>\n                        <v-row>\n                            <v-spacer></v-spacer>\n                            <!-- <v-btn class=\"mx-2\">\n                                <v-tooltip bottom>\n                                    <template v-slot:activator=\"{ on, attrs }\">\n                                        <v-icon\n                                        color=\"lime\"\n                                        dark\n                                        v-bind=\"attrs\"\n                                        v-on=\"on\"\n                                        >\n                                        mdi-book-open-variant\n                                        </v-icon>\n                                    </template>\n                                    <span>Items per page</span>\n                                </v-tooltip>\n                            </v-btn> -->\n                            <v-btn class=\"mx-2\" @click=\"toggleAddProperty\" text >\n                                <v-tooltip bottom>\n                                    <template v-slot:activator=\"{ on, attrs }\">\n                                        <v-icon\n                                            color=\"primary\"\n                                            dark\n                                            v-bind=\"attrs\"\n                                            v-on=\"on\"                                                                                       \n                                        >\n                                        mdi-plus-circle\n                                        </v-icon>\n                                    </template>\n                                    <span>Add Unit</span>\n                                </v-tooltip>\n                            </v-btn>\n                            <v-btn class=\"mx-2\" text >\n                                <v-tooltip bottom>\n                                    <template v-slot:activator=\"{ on, attrs }\">\n                                        <v-icon\n                                        color=\"light-blue\"\n                                        dark\n                                        v-bind=\"attrs\"\n                                        v-on=\"on\"\n                                        >\n                                        mdi-clipboard-list\n                                        </v-icon>\n                                    </template>\n                                    <span>List Units</span>\n                                </v-tooltip>\n                            </v-btn>\n                            <v-btn class=\"mx-2\" text >\n                                <v-tooltip bottom>\n                                    <template v-slot:activator=\"{ on, attrs }\">\n                                        <v-icon\n                                        color=\"green\"\n                                        dark\n                                        v-bind=\"attrs\"\n                                        v-on=\"on\"\n                                        >\n                                        mdi-download\n                                        </v-icon>\n                                    </template>\n                                    <span>Export</span>\n                                </v-tooltip>\n                            </v-btn>\n                        </v-row>\n                    </v-col>\n                </v-row>\n            </v-card-title>\n            <v-divider></v-divider>\n            <v-row class=\"px-10\">\n                <v-col>\n                    <v-data-table\n                        :headers=\"tableHeaders\"\n                        :items=\"unitList\"\n                        :search=\"searchUnit\"\n                        :items-per-page=\"10\"\n                        :page.sync=\"page\"\n                        class=\"elevation-0\"\n                        @page-count=\"pageCount = $event\" \n                        :loading=\"dataTableLoading\"\n                    >\n                        <template v-slot:[`item.attributes.vacant`]=\"{ item }\">\n                            <v-chip\n                                small\n                                :color=\"getOccupancyColor(item.attributes.vacant)\"\n                                dark\n                                class=\"white--text\"\n                            >\n                                {{ item.attributes.vacant}}\n                            </v-chip>\n                        </template>\n                        <template v-slot:[`item.delete`]=\"{ item }\">                            \n                            <v-icon\n                                medium\n                                color=\"red\"\n                                @click=\"showDeleteUnitModal(item)\"\n                                class=\"ml-2 text-center\"\n                            >\n                                mdi-delete-circle\n                            </v-icon>\n                        </template>\n                        <template v-slot:[`item.edit`]=\"{ item }\">   \n                            <v-icon\n                                medium\n                                color=\"blue\"\n                                class=\"mr-2 text-center\"\n                                @click=\"showEditUnitModal(item)\"\n                            >\n                                mdi-pencil-circle\n                            </v-icon>\n                        </template>\n                    </v-data-table>\n                </v-col>\n            </v-row>\n        </v-card>\n    </v-container>\n</template>\n\n<script>\nexport default {\n    name : 'Unit',\n    data () {\n        return {\n            title : 'Units',\n            unitList : [],\n            tableHeaders : [\n                {\n                    text : 'Name',\n                    align : 'Start',\n                    sortable : true,\n                    value : 'attributes.name'\n                },{\n                    text : 'Type',\n                    align : 'Start',\n                    sortable : true,\n                    value : 'relationships.type[0].attributes.type'\n                },{\n                    text : 'Occupancy',\n                    align : 'center',\n                    sortable : true,\n                    value : 'attributes.vacant'\n                },{\n                    text : 'Delete',\n                    value : 'delete',\n                    sortable : false\n                },\n                {\n                    text : 'Edit',\n                    value : 'edit',\n                    sortable : false\n                }\n            ]\n        }\n    },\n    computed : {\n        propertyID () {\n            return this.$store.state.property.selected.id ?? null;\n        }\n    },\n    watch : {\n        propertyID(newPropertyID, oldPropertyID) {\n            if(newPropertyID !== oldPropertyID) {\n                this.getPropertyUnits()\n            }\n        }\n    },\n    created () {\n        this.$nextTick(() => {\n            this.getPropertyUnits()\n            console.log(this.$store.state.unit.list)\n        });\n    },\n    methods : {\n        /**\n         * Get selected property id\n         */\n        getPropertyUnits() \n        {\n            this.$store.dispatch('unit/list', this.propertyID).then(\n                feedback => {\n                    let status = feedback.status\n                    if(status === 200) {\n                        this.unitList = feedback.data;\n                    }\n                },\n                fail => {\n                    console.log(fail.response.data)\n                }\n            )\n        },\n\n        /**\n         * Get occupancy color\n         */\n        getOccupancyColor(color) {\n            if(color === true) {\n                return \"success\"\n            } else {\n                return \"primary\"\n            }\n        }\n    }\n}\n</script>"]}]}