{"remainingRequest":"/var/www/html/real/vue/node_modules/vue-loader/lib/index.js??vue-loader-options!/var/www/html/real/vue/src/views/Tenant.vue?vue&type=script&lang=js&","dependencies":[{"path":"/var/www/html/real/vue/src/views/Tenant.vue","mtime":1612877924732},{"path":"/var/www/html/real/vue/node_modules/cache-loader/dist/cjs.js","mtime":1612338812780},{"path":"/var/www/html/real/vue/node_modules/babel-loader/lib/index.js","mtime":1612338812660},{"path":"/var/www/html/real/vue/node_modules/cache-loader/dist/cjs.js","mtime":1612338812780},{"path":"/var/www/html/real/vue/node_modules/vue-loader/lib/index.js","mtime":1612338817168}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Tenant.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6IA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA","file":"Tenant.vue","sourceRoot":"src/views","sourcesContent":["<template>\n    <v-container>\n        <v-snackbar\n            v-model=\"showSnackbar\"\n            :color=\"snackbarColor\"\n            top\n            timeout=\"-1\"\n            tag=\"div\"\n        >\n            {{ snackbarText }}\n\n            <template v-slot:action=\"{ attrs }\">\n                <v-icon\n                    v-bind=\"attrs\"\n                    @click=\"showSnackbar = false\"\n                    class=\"white--text\"\n                >\n                mdi-close\n                </v-icon>\n            </template>\n        </v-snackbar>\n        <v-card>\n            <v-card-title\n                class=\"px-10 py-0\"\n            >\n                <v-row>\n                    <v-col\n                        lg=\"3\" \n                        md=\"3\"\n                        sm=\"0\"\n                        class=\"d-none d-md-flex\"\n                    >\n                         <v-list-item two-line>\n                            <v-list-item-content>\n                                <v-list-item-title class=\"grey--text\">{{ title }}</v-list-item-title>\n                            </v-list-item-content>\n                        </v-list-item>\n                    </v-col>\n                    <v-col>\n                        <v-row>\n                            <v-spacer></v-spacer>\n                            <v-btn class=\"mx-2\" @click=\"toggleAddTenant\" text>\n                                <v-tooltip bottom>\n                                    <template v-slot:activator=\"{ on, attrs }\">\n                                        <v-icon\n                                            color=\"primary\"\n                                            dark\n                                            v-bind=\"attrs\"\n                                            v-on=\"on\"                                                                                       \n                                        >\n                                        mdi-plus-circle\n                                        </v-icon>\n                                    </template>\n                                    <span>New Tenant</span>\n                                </v-tooltip>\n                            </v-btn>\n                            <v-btn class=\"mx-2\" text>\n                                <v-tooltip bottom>\n                                    <template v-slot:activator=\"{ on, attrs }\">\n                                        <v-icon\n                                        color=\"light-blue\"\n                                        dark\n                                        v-bind=\"attrs\"\n                                        v-on=\"on\"\n                                        >\n                                        mdi-clipboard-list\n                                        </v-icon>\n                                    </template>\n                                    <span>List Tenants</span>\n                                </v-tooltip>\n                            </v-btn>\n                            <v-btn class=\"mx-2\" text>\n                                <v-tooltip bottom>\n                                    <template v-slot:activator=\"{ on, attrs }\">\n                                        <v-icon\n                                        color=\"green\"\n                                        dark\n                                        v-bind=\"attrs\"\n                                        v-on=\"on\"\n                                        >\n                                        mdi-download\n                                        </v-icon>\n                                    </template>\n                                    <span>Download</span>\n                                </v-tooltip>\n                            </v-btn>\n                        </v-row>\n                    </v-col>\n                </v-row>\n            </v-card-title>\n            <v-divider></v-divider>\n            <v-row class=\"px-10\">\n                <v-col>\n                    <v-data-table\n                        :headers=\"tableHeaders\"\n                        :items=\"unitList\"\n                        :search=\"searchTenant\"\n                        :items-per-page=\"10\"\n                        :page.sync=\"page\"\n                        class=\"elevation-0\"\n                        @page-count=\"pageCount = $event\" \n                        :loading=\"dataTableLoading\"\n                    >\n                        <template v-slot:[`item.attributes.gender`]=\"{ item }\">\n                            <v-chip\n                                small\n                                :color=\"getGenderColor(item.attributes.gender)\"\n                                dark\n                                class=\"white--text\"\n                            >\n                                {{ item.attributes.gender}}\n                            </v-chip>\n                        </template>\n                        <template v-slot:[`item.delete`]=\"{ item }\">                            \n                            <v-icon\n                                medium\n                                color=\"red\"\n                                @click=\"showDeleteTenantModal(item)\"\n                                class=\"ml-2 text-center\"\n                            >\n                                mdi-delete-circle\n                            </v-icon>\n                        </template>\n                        <template v-slot:[`item.edit`]=\"{ item }\">   \n                            <v-icon\n                                medium\n                                color=\"blue\"\n                                class=\"mr-2 text-center\"\n                                @click=\"showEditTenantModal(item)\"\n                            >\n                                mdi-pencil-circle\n                            </v-icon>\n                        </template>\n                    </v-data-table>\n                </v-col>\n            </v-row>\n        </v-card>\n    </v-container>\n</template>\n\n<script>\nexport default {\n    name : 'Tenant',\n    data () {\n        return {\n            title : 'Tenant',\n            tenantList : [],\n            tableHeaders : [\n                {\n                    text : 'Names',\n                    align : 'Start',\n                    sortable : true,\n                    value : 'attributes.name'\n                },{\n                    text : 'Gender',\n                    align : 'Start',\n                    sortable : true,\n                    value : 'relationships.type[0].attributes.type'\n                },{\n                    text : 'Status',\n                    align : 'center',\n                    sortable : true,\n                    value : 'attributes.vacant'\n                },{\n                    text : 'From',\n                    align : 'start',\n                    sortable : true,\n                    value : 'attributes.startDate'\n                },{\n                    text : 'Delete',\n                    value : 'delete',\n                    sortable : false\n                },\n                {\n                    text : 'Edit',\n                    value : 'edit',\n                    sortable : false\n                }\n            ]\n        }\n    },\n    computed : {\n        propertyID () {\n            return this.$store.state.property.selected.id ?? null;\n        }\n    },\n    watch : {\n        propertyID(newPropertyID, oldPropertyID) {\n            if(newPropertyID !== oldPropertyID) {\n                this.getPropertyTenants()\n            }\n        }\n    },\n    created () {\n        this.$nextTick(() => {\n            this.getPropertyTenants()\n            \n        });\n    },\n    methods : {\n        /**\n         * Get selected property id\n         */\n        getPropertyTenants() \n        {\n            console.log(this.propertyID)\n            this.$store.dispatch('tenant/list', this.propertyID).then(\n                feedback => {\n                    console.log(feedback)\n                    let status = feedback.status\n                    if(status === 200) {\n                        this.tenantList = feedback.data;\n                    }\n                },\n                fail => {\n                    console.log(fail)\n                }\n            )\n        },\n\n        /**\n         * Get occupancy color\n         */\n        getGenderColor(sex) {\n            if(sex === 'Male') {\n                return \"blue\"\n            } else if (sex === 'Female') {\n                return \"warning\"\n            } else {\n                return \"brown\"\n            }\n        }\n    }\n\n}\n</script>"]}]}